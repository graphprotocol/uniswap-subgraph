{
  "name": "uniswap-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/graphprotocol/uniswap-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "watch-local": "graph deploy $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020 --watch",
    "deploy-staging": "graph deploy $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME --debug --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "deploy": "graph deploy graphprotocol/uniswap --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.12.0",
    "@graphprotocol/graph-ts": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.0.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
